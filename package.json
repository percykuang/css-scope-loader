{
  "name": "css-scope-loader",
  "version": "0.0.3",
  "description": "A webpack loader that adds scope to css in react projects",
  "keywords": [
    "webpack",
    "loader",
    "css",
    "less",
    "react",
    "tsx",
    "jsx"
  ],
  "author": "Percy Kuang",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
