    "@types/babel__generator": "^7.6.8",
    "@types/babel__traverse": "^7.20.5",
    "@types/css": "^0.0.37",
    "@types/jest": "^29.5.11",
    "@types/node": "^22.8.1",
    "@types/webpack": "^5.28.5",
    "css": "^3.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
